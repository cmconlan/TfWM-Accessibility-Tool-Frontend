{
  "name": "transport-accessibility-tool",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npx license-checker --json --out public/licenses.json --customPath public/license_format.json --production --direct --relativeLicensePath && vue-cli-service build",
    "cypress:open": "cypress open",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "buefy": "^0.8.20",
    "core-js": "^3.9.1",
    "epic-spinners": "^1.1.0",
    "leaflet": "^1.7.1",
    "luxon": "^1.28.1",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "tailwindcss": "^1.9.6",
    "vue": "^2.6.12",
    "vue-color": "^2.8.1",
    "vue-datetime": "^1.0.0-beta.14",
    "vue-inline-datepicker": "^1.0.7",
    "vue-material-design-icons": "^4.11.0",
    "vue-multipane": "^0.9.5",
    "vue-multiselect": "^2.1.6",
    "vue-numeric": "^2.3.0",
    "vue-router": "^3.5.1",
    "vue2-leaflet": "^2.6.0",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.6.2",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-plugin-vuex": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "cypress": "^6.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.8.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "rules": {
      "vue/no-side-effects-in-computed-properties": "warning"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
